{
  "version": 2,
  "builds": [
    { "src": "auth/route.js", "use": "@vercel/node" },
    { "src": "bookings/route.js", "use": "@vercel/node" },
    { "src": "bookings/[id]/route.js", "use": "@vercel/node" },
    { "src": "bookings/[id]/confirm/route.js", "use": "@vercel/node" },
    { "src": "bookings/[id]/cancel/route.js", "use": "@vercel/node" },
    { "src": "restaurants/route.js", "use": "@vercel/node" },
    { "src": "restaurants/search/route.js", "use": "@vercel/node" },
    { "src": "businesses/route.js", "use": "@vercel/node" },
    { "src": "notifications/route.js", "use": "@vercel/node" },
    { "src": "events/route.js", "use": "@vercel/node" },
    { "src": "events/[id]/route.js", "use": "@vercel/node" },
    { "src": "events/[id]/participants/route.js", "use": "@vercel/node" }
  ],
  "rewrites": [
    { "source": "/api/auth", "destination": "/auth/route.js" },
    { "source": "/api/bookings", "destination": "/bookings/route.js" },
    { "source": "/api/bookings/:id", "destination": "/bookings/[id]/route.js" },
    { "source": "/api/bookings/:id/confirm", "destination": "/bookings/[id]/confirm/route.js" },
    { "source": "/api/bookings/:id/cancel", "destination": "/bookings/[id]/cancel/route.js" },
    { "source": "/api/restaurants", "destination": "/restaurants/route.js" },
    { "source": "/api/restaurants/search", "destination": "/restaurants/search/route.js" },
    { "source": "/api/businesses", "destination": "/businesses/route.js" },
    { "source": "/api/notifications", "destination": "/notifications/route.js" },
    { "source": "/api/events", "destination": "/events/route.js" },
    { "source": "/api/events/:id", "destination": "/events/[id]/route.js" },
    { "source": "/api/events/:id/participants", "destination": "/events/[id]/participants/route.js" }
  ]
}
